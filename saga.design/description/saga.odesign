  <ownedViewpoints name="treeView" modelFileExtension="saga">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="TotalView" domainClass="saga::System" enablePopupBars="true">
      <validationSet name="validate sagas">
        <ownedRules xsi:type="validation:SemanticValidationRule" name="containment" message="Step is not contained in container... eh what?" targetClass="saga::SagaStep">
          <audits auditExpression="aql:self.eContainer().steps->includes(self)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="saga step siblings" level="ERROR" message="sibilng step siblings are of different containers" targetClass="saga::SagaStep">
          <audits auditExpression="aql:self.eContainer().oclAsType(saga::Saga).steps->includes(self.nextStep) or self.nextStep == null"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="not self " level="ERROR" message="self can not be next step " targetClass="saga::SagaStep">
          <audits auditExpression="aql:self.nextStep != self"/>
        </ownedRules>
      </validationSet>
      <layout xsi:type="description_1:CustomLayoutConfiguration" id="org.eclipse.elk.layered" label="ELK Layered" description="Layer-based algorithm provided by the Eclipse Layout Kernel. Arranges as many edges as possible into one direction by placing nodes into subsequent layers. This implementation supports different routing styles (straight, orthogonal, splines); if orthogonal routing is selected, arbitrary port constraints are respected, thus enabling the layout of block diagrams such as actor-oriented models or circuit schematics. Furthermore, full layout of compound graphs with cross-hierarchy edges is supported when the respective option is activated on the top level.">
        <layoutOptions xsi:type="description_1:EnumLayoutOption" id="org.eclipse.elk.direction">
          <targets>PARENT</targets>
          <value name="UNDEFINED"/>
        </layoutOptions>
      </layout>
      <defaultLayer name="Default">
        <toolSections name="create_saga_tools" label="Saga Tools">
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="create_saga" label="create new Saga" nodeMappings="//@ownedViewpoints[name='treeView']/@ownedRepresentations[name='TotalView']/@additionalLayers[name='saga_layer']/@nodeMappings[name='SagaNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="saga::Saga" referenceName="sagas">
                  <subModelOperations xsi:type="tool:SetValue" featureName="id" valueExpression="aql:'saga'+container.sagas->filter(saga::Saga)->size()"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="create_step" label="create new SagaStep" nodeMappings="//@ownedViewpoints[name='treeView']/@ownedRepresentations[name='TotalView']/@additionalLayers[name='saga_layer']/@nodeMappings[name='SagaStepNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:container.sagas->last()">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="saga::SagaStep" referenceName="steps">
                  <subModelOperations xsi:type="tool:SetValue" featureName="id" valueExpression="aql:'step'+container.eAllContents()->size()"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:EdgeCreationDescription" name="create_edge_step_task" label="Connect to Tasks" edgeMappings="//@ownedViewpoints[name='treeView']/@ownedRepresentations[name='TotalView']/@additionalLayers[name='saga_layer']/@edgeMappings[name='edge_saga_task'] //@ownedViewpoints[name='treeView']/@ownedRepresentations[name='TotalView']/@additionalLayers[name='container_saga_layer']/@edgeMappings[name='edge_saga_task']">
            <sourceVariable name="source_step"/>
            <targetVariable name="target_task"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:source_step">
                <subModelOperations xsi:type="tool:SetValue" featureName="task" valueExpression="var:target_task"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:EdgeCreationDescription" name="create_edge_step_face" label="Connect to Interface" edgeMappings="//@ownedViewpoints[name='treeView']/@ownedRepresentations[name='TotalView']/@additionalLayers[name='saga_layer']/@edgeMappings[name='edge_saga_interface'] //@ownedViewpoints[name='treeView']/@ownedRepresentations[name='TotalView']/@additionalLayers[name='container_saga_layer']/@edgeMappings[name='edge_saga_interface']">
            <sourceVariable name="source_step"/>
            <targetVariable name="target_iface"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
